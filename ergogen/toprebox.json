points:
  key.padding: u
  zones:

    matrix:
      anchor:
        rotate: 5
      columns:
        outerpinky:
        pinky:
        ring:
          key.splay: -5
          key.origin: [-12, -19]
          key.stagger: 12
        middle:
          key.stagger: 5
        index:
          key.splay: -5
          key.stagger: -6
        inner:
          key.stagger: -2
      rows:
        bottom:
        home:
        top:
    thumbfan:
      anchor:
        ref: matrix_inner_bottom
        shift: [-1.75u, -19]
      columns:
        outer:
          key:
            shift: [-0.3u]
            rotate: 
        middle:
          key:
            width: 1.25u
            shift: [-0.15u]
        home:
          key.spread: 1u 
          key.origin: [-11.75, -9]
  rotate: -20
  mirror: &mirror
    ref: matrix_inner_top
    shift: [10.5,0]
    distance: 22
outlines:
      topre:
        - what: rectangle
          where: true
          size: [14.6,14]
          bevel: 1
      pcb:
        - what: polygon
          operation: stack
          points:
            - ref: matrix_outerpinky_bottom
              shift: [-0.5u,-0.5u]
            - ref: matrix_outerpinky_top
              shift: [-0.5u,0.5u]
            - ref: matrix_ring_top
              shift: [u,u]

            - ref: mirror_matrix_ring_top
              shift: [u,u]
            - ref: mirror_matrix_outerpinky_top
              shift: [-0.5u,0.5u]
            - ref: mirror_matrix_outerpinky_bottom
              shift: [-0.5u,-0.5u]

            - ref: mirror_thumbfan_outer
              shift: [-0.5u,-0.8u]
            - ref: mirror_thumbfan_home
              shift: [0.5u,-0.5u]

            - ref: thumbfan_home
              shift: [0.5u,-0.5u]
            - ref: thumbfan_outer
              shift: [-0.5u,-0.8u]

      mounting_holes:
        - what: circle
          radius: 1
          operation: stack
          where:
            ref: [matrix_ring_top]
            shift: [10,0]
        

      Topre_Plate:
        - name: pcb
        - operation: subtract
          name: topre

      topreplate:
        - name: Topre_Plate
        - operation: subtract
          name: mounting_holes

